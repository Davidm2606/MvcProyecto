@model Empleados
@{
    ViewData["Title"] = "Visualizar Empleados";
}


<div>
    <h1 class="display-4">VISUALIZAR Empleados</h1>

    <form method="post">

        <div class="form-group">
            <label for="exampleInputEmail1">Cedula</label>
            <input asp-for="Cedula" class="form-control" placeholder="Ingrese la cedula">
        </div>

        <div class="form-group">
            <label for="exampleInputEmail1">Nombre</label>
            <input asp-for="Nombre" class="form-control" placeholder="Ingrese el nombre">
        </div>

        <div class="form-group">
            <label for="exampleInputEmail1">Apellido</label>
            <input asp-for="Apellido" class="form-control" placeholder="Ingrese el apellido">
        </div>

        <div class="form-group">
            <label for="exampleInputEmail1">Edad</label>
            <input asp-for="Edad" class="form-control" placeholder="Ingrese la edad">
        </div>

        <div class="form-group">
            <a asp-action="Index1" class="btn btn-warning">Volver</a>
        </div>

    </form>

</div>
